<%= render 'group_profile', group: @group %>

<div>
  <% @items.each do |item| %>
    <%= link_to item_path(item.id) do %>
      <%= image_tag item.image, size: "150x150" %>
    <% end %>
    <p>投稿ユーザー画像：<%= image_tag item.user.get_profile_image(100,100) %></p>
    <p>タイトル：<%= item.title %></p>
    <p>説明：<%= item.body %></p>
    <p>ユーザーネーム：<%= item.user.name %></p>
  <% end %>
</div>