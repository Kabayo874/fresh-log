<%= render 'profile', user: @user %>

<div>
  <% @items.each do |item| %>
    <div>
      <%= link_to item_path(item.id) do %>
        <%= image_tag item.image, size: "150x150" %>
      <% end %>
      <p>タイトル：<%= item.title %></p>
      <p>説明：<%= item.body %></p>
      <p>ユーザーネーム：<%= item.user.name %></p>
    </div>
  <% end %>
